    "version": "0.2.0",
    "configurations": [
        {
            "name": "mongo2xlsx debug",
            "type": "debugpy",
            "request": "launch",
            "program": "mongo2xlsx/mongo2xlsx.py",
            "args": [
                "--out",
                "doc.xlsx",
                "--db",
                "test"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "type": "bashdb",
            "request": "launch",
            "name": "fix_symlinks_source.sh",
            "cwd": "${workspaceFolder}/fix_symlinks_source/test-dir/",
            "program": "${workspaceFolder}/fix_symlinks_source/test-dir/fix_symlinks_source.sh",
            "args": ["src"]
        }
    ]
